{
  "expo": {
    "name": "PlantPalNew",
    "slug": "PlantPalNew",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.markplants.PlantPalNew",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to identify plants.",
        "NSUserNotificationUsageDescription": "This app uses notifications to remind you to water your plants."
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.markplants.PlantPalNew",
      "versionCode": 1,
      "permissions": [
        "NOTIFICATIONS",
        "SCHEDULE_EXACT_ALARM"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "4d99ca4d-c3e5-4698-a9b6-253e47d7aa17"
      }
    },
    "plugins": [
      "expo-notifications"
    ]
  }
}
